{
  "name": "quick-note-popup",
  "version": "1.0.0",
  "description": "A tool for quick opening a simple list of notes, where you can type a new note and save it.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . --icon=\"./app_icon.icns\" --overwrite --out=build",
    "buildWin32": "electron-packager . --icon=\"./app_icon.icns\" --overwrite --out=build --platform=win32 --arch=x64",
    "lint": "eslint *.js",
    "lint-fix": "eslint --fix *.js"
  },
  "repository": "https://github.com/shlomo666/quick-note-popup",
  "keywords": [],
  "author": "GitHub",
  "license": "MIT",
  "devDependencies": {
    "electron": "^8.2.5",
    "electron-packager": "^14.2.1",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.0"
  },
  "productName": "Quick Note Popup",
  "dependencies": {
    "lodash.clamp": "^4.0.3"
  }
}
